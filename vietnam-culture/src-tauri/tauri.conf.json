{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "enterVN",
  "version": "0.1.0",
  "identifier": "enterVN.VNPuzzle",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "enterVN",
        "fullscreen": false,
        "maximized": true,
        "decorations": true,
        "resizable": true,
        "maximizable": true,
        "minimizable": true,
        "closable": true,
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/VietNam.ico"
    ]
  }
}
